{
  "name": "konsum-frontend",
  "version": "0.0.0-semantic-release",
  "repository": {
    "type": "git",
    "url": "https://github.com/k0nsti/konsum-frontend.git"
  },
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "webpack --config webpack.production.js",
    "start": "webpack-dev-server --open",
    "package": "npm-pkgbuild -t /data/binaries/arch-linux/packages pkgbuild pacman makepkg"
  },
  "dependencies": {
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@vue/babel-preset-app": "^3.4.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-pkgbuild": "^5.0.4",
    "semantic-release": "^15.0.0",
    "vue-loader": "^15.6.4",
    "vue-template-compiler": "^2.6.5",
    "webpack-cli": "^3.2.3",
    "webpack": "^4.29.5",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "pacman": {
    "groups": "home-automation",
    "content": {
      "/services/konsum/docroot/main.js": "dist/main.js",
      "/services/konsum/docroot/index.html": "dist/index.html"
    },
    "depends": {
      "konsum": ""
    }
  }
}
